{
  "name": "@reedchan/onetime",
  "version": "8.0.5",
  "description": "Ensure a function is only called once.\nForked from sindresorhus/onetime, with native TypeScript support.",
  "license": "MIT",
  "repository": "reedchan7/onetime",
  "homepage": "https://github.com/reedchan7/onetime",
  "funding": "https://github.com/sponsors/reedchan7",
  "author": {
    "name": "Reed Chan",
    "email": "reedchan7@gmail.com",
    "url": "https://reedchan.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "npx tsc && npx tsc --module esnext --outDir dist/esm && mv dist/esm/index.js dist/index.esm.js && rm -rf dist/esm",
    "dev": "tsc --watch",
    "test": "pnpm run build && vitest run",
    "test:watch": "vitest",
    "prettier": "npx prettier --write *.ts",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "once",
    "function",
    "one",
    "onetime",
    "func",
    "fn",
    "single",
    "call",
    "called",
    "prevent"
  ],
  "devDependencies": {
    "@types/node": "^24.5.2",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "xo": "^1.2.2"
  }
}
